  "dependencies": {
    "viem": "^2.7.1",
    "@deadw3/types": "workspace:*",
    "@deadw3/config": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}
