{
  "name": "@deadw3/clanker-sdk",
  "version": "0.1.0",
  "private": true,
  "description": "Wrapper SDK for Clanker token deployment interactions",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
